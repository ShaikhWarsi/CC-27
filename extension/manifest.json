{
    "manifest_version": 3,
    "name": "Antigravity Phish-Pish",
    "version": "1.0",
    "description": "Agent-grade phishing detection and URL analysis.",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "storage",
        "scripting",
        "cookies"
    ],
    "host_permissions": [
        "http://localhost:8000/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "content.css"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "action": {
        "default_title": "Fish-Pish"
    }
}